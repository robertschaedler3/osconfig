  "AllowDatagramProcessingOnWinServer": {
    "scope": "json",
    "prefix": "AllowDatagramProcessingOnWinServer",
    "description": "This setting controls datagram processing for Network Protection is enabled on Server",
    "body": [
      "{",
      "\t\"name\": \"${1:AllowDatagramProcessingOnWinServer}\",",
      "\t\"type\": \"Microsoft.Windows/Registry\",",
      "\t\"properties\": {",
      "\t\t\"key\": \"HKEY_LOCAL_MACHINE\\\\\\SOFTWARE\\\\\\Policies\\\\\\Microsoft\\\\\\Windows Defender\\\\\\NIS\\\\\\Consumers\\\\\\IPS\",",
      "\t\t\"value\": \"AllowDatagramProcessingOnWinServer\",",
      "\t\t\"type\": \"REG_DWORD\"",
      "\t},",
      "\t\"data\": ${2:0}",
      "}$0"
    ]
  },
